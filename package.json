{
  "name": "crest",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server.js",
    "start:dev": "set DEBUG=check && nodemon --exec babel-node ./server/server.js",
    "build": "npm run clean && npm run babel-build",
    "test": "nyc mocha --require @babel/register --recursive server/tests/ --timeout 30000 --exit",
    "create-table": "node server/db/db.js createUserTable && node server/db/db.js createAccountTable && node server/db/db.js createTransactionTable",
    "drop-table": "node server/db/db.js dropUserTable && node server/db/db.js dropAccountTable && node server/db/db.js dropTransactionTable",
    "seeder": "node server/db/seeder.js insertAccount && node server/db/seeder.js insertAccount",
    "generate-lcov": "nyc report --reporter=text-lcov > lcov.info",
    "coveralls-coverage": "coveralls < lcov.info",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "babel-build": "babel server --ignore ./server/tests/* -d dist",
    "clean": "rm -rf dist && mkdir dist",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alpha1202/crest.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Alpha1202/crest/issues"
  },
  "homepage": "https://github.com/Alpha1202/crest#readme",
  "dependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^3.0.5",
    "body-parser": "^1.18.3",
    "config": "^3.0.1",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.1",
    "make-runnable": "^1.3.6",
    "moment": "^2.24.0",
    "pg": "^7.10.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
    "babel-watch": "^7.0.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "coveralls": "^3.0.3",
    "cross-env": "^5.2.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "mocha": "^6.0.2",
    "nodemon": "^1.18.10",
    "nyc": "^13.3.0"
  },
  "engines": {
    "node": "10.15.0"
  }
}
